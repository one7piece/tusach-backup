<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	.floatLeft {
		float: left;
	}
	
	</ui:style>
	<g:HTMLPanel>
		<g:FlowPanel width="100%" height="100%">
			<g:FlowPanel>
				<g:DisclosurePanel open="true">
					<g:header>New DisclosurePanel</g:header>
					<g:VerticalPanel width="5cm" height="4cm">
						<g:Label text="Name"/>
					</g:VerticalPanel>
				</g:DisclosurePanel>
			</g:FlowPanel>
			<g:DockLayoutPanel unit="EM" width="100%" height="100px">
				<g:north size="7.7">
					<g:Label text="Header message"/>
				</g:north>
			</g:DockLayoutPanel>
		</g:FlowPanel>
	</g:HTMLPanel>
</ui:UiBinder> 